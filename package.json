{
  "name": "another-fullstack-starter",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,cjs,mjs,json}\"",
    "validate": "turbo run lint typecheck build",
    "generate": "turbo run generate",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "deps:update": "bunx npm-check-updates -ui -ws --pre beta"
  },
  "devDependencies": {
    "eslint": "8.48.0",
    "gitzy": "4.0.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "bun@1.0.9",
  "workspaces": [
    "apps/*",
    "libs/*"
  ]
}
